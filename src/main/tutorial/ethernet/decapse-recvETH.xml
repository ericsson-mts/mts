<?xml version="1.0" encoding="UTF-8" ?> 
<scenario>	
	<receiveMessageETHERNET>
		<parameter name="[data1]" operation="protocol.setFromMessage" value="data.binary" />
	</receiveMessageETHERNET>
	
	<parameter name="[header]" operation="binary.subbinary" value="[data1]" value2="0" value3="10" />
	<parameter name="[checksum]" operation="binary.subbinary" value="[data1]" value2="10" value3="2" />
	<parameter name="[ipsrc]"  operation="binary.subbinary" value="[data1]" value2="12" value3="4" />
	<parameter name="[ipdst]"  operation="binary.subbinary" value="[data1]" value2="16" value3="4" />
	<parameter name="[udp]" operation="binary.subbinary" value="[data1]" value2="20" value3="8" />
	<parameter name="[sip]"  operation="binary.subbinary" value="[data1]" value2="28" value3="338" />
	
	<!-- Don't forget to replace checksum, IPSRC and IPDST in binary string below (byte 10 to 20) by values you have set up in ETHERNET sent frame -->
	<test parameter="[header]" condition="binary.equals" value="4500016e000040004011"/>
	<test parameter="[checksum]" condition="binary.equals" value="b67e"/>
	<test parameter="[ipsrc]" condition="binary.equals" value="[localBinaryIP]"/>
	<test parameter="[ipdst]" condition="binary.equals" value="[localBinaryIP]"/>
	<test parameter="[udp]" condition="binary.equals" value="1b9e13c4015ad3b2"/>
	<test parameter="[sip]" condition="binary.equals" value=""/>

	
	<finally>
		<removeProbeETHERNET name="capture" />
	</finally>
</scenario>