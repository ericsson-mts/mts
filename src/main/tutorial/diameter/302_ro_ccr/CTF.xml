          <avp code="Destination-Realm:283" mandatory="true" type="DiameterIdentity" value="[localRealmDIAM]"/>            

          <!-- The Auth-Application-Id AVP is used in order to advertise support of the Authentication and Authorization portion of an application. -->
          <!--avp code="Auth-Application-Id:258" mandatory="true" type="Unsigned32" value="4"/-->
          <avp code="Acct-Application-Id:259" mandatory="true" type="Unsigned32" value="Diameter Credit Control Application:4"/>

          <!-- The Service-Context-Id AVP contains a unique identifier of the Diameter Credit Control service specific document that applies to the request. -->
          <avp code="Service-Context-Id:461" mandatory="true" type="UTF8String" value="ToBeCompleted"/>

          <!-- The CC-Request-Type AVP defines the transfer type: event for event based charging and initial, update, terminate for session based charging. -->
          <!-- Possible enumerated values : INITIAL_REQUEST(1) / UPDATE_REQUEST(2) / TERMINATION_REQUEST(3) / EVENT_REQUEST(4). -->
          <avp code="CC-Request-Type:416" mandatory="true" type="Enumerated" value="INITIAL_REQUEST:1"/>

          <!-- The CC-Request-Number AVP contains the sequence number of the transferred messages. -->
          <avp code="CC-Request-Number:415" mandatory="true" type="Unsigned32" value="0"/>            

          <!-- The User-Name AVP contains the User-Name, in a format consistent with the Network Access Identifier (NAI) specification. -->
          <avp code="User-Name:1" mandatory="true" type="UTF8String" value="ToBeCompleted"/>

          <!-- Not used avp code="CC-Sub-Session-Id:419" type="Unsigned64" value="0"/-->
          <!-- Not used avp code="Acct-Multi-Session-Id" mandatory="true" type="UTF8String" value="ToBeCompleted"/-->

          <!-- The Origin-State-Id AVP is a monotonically increasing value that is advanced whenever a Diameter entity restarts with loss of previous state. -->
          <avp code="Origin-State-Id:278" mandatory="true" type="Unsigned32" value="0"/>

          <!-- The Event-Timestamp MAY be included in an Accounting-Request and Accounting-Answer messages to record the time that the reported event occurred, in seconds sinceJanuary 1, 1900 00:00 UTC. -->
          <avp code="Event-Timestamp:55" mandatory="true" type="Time" value="0"/>


          <avp code="Proxy-Info:284">
             <avp code="Proxy-Host:280" mandatory="true" private="false" type="DiameterIdentity" value="ToBeCompleted"/>
             <avp code="Proxy-State:33" mandatory="true"  private="false" type="OctetString" value="ToBeCompleted"/>
          </avp>
          <avp code="Proxy-Info:284">
