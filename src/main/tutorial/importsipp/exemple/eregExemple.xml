<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">
<!-- Regular expressions example -->
<scenario name="ereg example">
  <send retrans="500">
    <![CDATA[

      INVITE sip:[service]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: sipp <sip:sipp@[local_ip]:[local_port]>;tag=[pid]SIPpTag02[call_number]
      To: sut <sip:[service]@[remote_ip]:[remote_port]>
      Call-ID: [call_id]
      CSeq: 1 INVITE
      Contact: sip:sipp@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[media_ip_type] [media_ip]
      t=0 0
      m=audio [media_port] RTP/AVP 0
      a=rtpmap:0 PCMU/8000

    ]]>
  </send>
  <!-- Example search_in msg -->
  <recv response="200" start_rtd="true">
  <action>
      <ereg regexp="[0-9]{1,3}[.][0-9]{1,3}[.][0-9]{1,3}[.][0-9]{1,3}[:][0-9]{1,5}" 
            search_in="msg" 
            check_it="true" 
            assign_to="1"/>
  </action>
  </recv>
  <!-- Example search_in hdr -->
  <recv response="200" start_rtd="true">
  <action>
      <ereg regexp=".*" 
            search_in="hdr" 
            header="Contact:" 
            check_it="true" 
            assign_to="6"/>
  </action>
  </recv>
  <!-- Example search_in hdr with startline = true -->
  <recv response="200" start_rtd="true">
  <action>
      <ereg regexp="o=([[:alnum:]]*) ([[:alnum:]]*) ([[:alnum:]]*)"
            search_in="msg" 
            check_it="true" 
			startline = "true"
            assign_to="3,4,5,8"/>
  </action>
  </recv>
  <!-- Many eregs together -->
  <recv response="200" start_rtd="true">
  <action>
       <ereg search_in="var" 
             variable="8"/>
	   	 
	   <ereg regexp=".*" 
             search_in="hdr" 
             header="Contact:" 
             check_it="true" 
             assign_to="6"/>
			 
	   <ereg regexp="o=([[:alnum:]]*) ([[:alnum:]]*) ([[:alnum:]]*)"
             search_in="msg" 
             check_it="true" 
             assign_to="3,4,5,8"/>
  </action>
  </recv>
</scenario>