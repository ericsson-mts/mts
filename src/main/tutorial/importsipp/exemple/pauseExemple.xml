<?xml version="1.0" encoding="ISO-8859-2" ?>
<scenario name="pause example">
<!-- Normal <pause> -->
<pause milliseconds="20"/>
<!-- With no attribute-->
<pause />
<!-- With no attribute-->
<pause variable="20"/>
<!-- Fixed distribution-->
<pause distribution="fixed" value="20"/>
<!-- Uniform distribution-->
<pause distribution="uniform" max="50" min="10"/>
<!-- Normal distribution-->
<pause distribution="normal" mean="50" stdev="5"/>
</scenario>