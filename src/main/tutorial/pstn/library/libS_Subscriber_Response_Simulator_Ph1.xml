<if>
<condition>
<test parameter="[3_5_3_Rallonge_CANCEL_Tempo_only_for_TOKAY]" condition="exists" />
<test parameter="[3_5_3_CANCEL_Tempo_Value_rallongee]" condition="exists" />	  		  		    		  		  	
