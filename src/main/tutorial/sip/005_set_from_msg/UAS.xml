<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/scenario.xsd -->

<scenario>

    <receiveMessageSIP name="Wait INVITE" request="true" type="INVITE">
    
    <parameter name="[message]" operation="protocol.setFromMessage" value="message"/>
    <test parameter="[message]" condition="string.contains" value="SIP/2.0"/>
    
    <parameter name="[listenpoint]" operation="protocol.setFromMessage" value="listenpoint"/>
    <test parameter="[listenpoint]" condition="string.contains" value="0.0.0.0"/>
    <test parameter="[listenpoint]" condition="string.contains" value="[localPortSIP]"/>
    <parameter name="[listenpointXML]" operation="protocol.setFromMessage" value="listenpoint.xml"/>
    <test parameter="[listenpointXML]" condition="string.contains" value="LISTENPOINT"/>
    <test parameter="[listenpointXML]" condition="string.contains" value="0.0.0.0"/>
    <test parameter="[listenpointXML]" condition="string.contains" value="[localPortSIP]"/>
    
    <parameter name="[channel]" operation="protocol.setFromMessage" value="channel"/>
    <test parameter="[channel]" condition="string.contains" value="[localHostSIP]"/>
    <test parameter="[channel]" condition="string.contains" value="[localPortSIP]"/>
    <test parameter="[channel]" condition="string.contains" value="[remoteHostSIP]"/>
    <test parameter="[channel]" condition="string.contains" value="[remotePortSIP]"/>
    <test parameter="[channel]" condition="string.contains" value="UDP"/>
    <parameter name="[channelXML]" operation="protocol.setFromMessage" value="channel.xml"/>
    <test parameter="[channelXML]" condition="string.contains" value="CHANNEL"/>
    <test parameter="[channelXML]" condition="string.contains" value="[localHostSIP]"/>
    <test parameter="[channelXML]" condition="string.contains" value="[localPortSIP]"/>
    <test parameter="[channelXML]" condition="string.contains" value="[remoteHostSIP]"/>
    <test parameter="[channelXML]" condition="string.contains" value="[remotePortSIP]"/>
    <test parameter="[channelXML]" condition="string.contains" value="UDP"/>
    
    <parameter name="[receivedMessageLength]" operation="protocol.setFromMessage" value="message.length"/>
    
    <!--test parameter="[receivedMessageLength]" condition="string.equals" value="2155"/--> <!-- Jain SIP stack -->
    <!--test parameter="[receivedMessageLength]" condition="string.equals" value="2294"/--> <!-- Light SIP stack -->
    <!-- because of the nsn.ADD_CONTENT_LENGTH = "3" config parameter -->
    <parameter name="[receivedMessageLength]" operation="number.add" value="[receivedMessageLength]" value2="6"/>
    <test parameter="[receivedMessageLength]" condition="number.equals" value="[testcase:sentMessageLength]"/>
    
    <parameter name="[timestamp1]" operation="system.timestamp" value="1970"/>
    <parameter name="[timestamp2]" operation="protocol.setFromMessage" value="message.timestamp"/>  
    <parameter name="[diffTimestamp]" operation="number.substract" value="[timestamp2]" value2="[timestamp1]"/>
    <test parameter="[diffTimestamp]" condition="number.lowerThan" value="2000"/>
    
    <parameter name="[text]" operation="protocol.setFromMessage" value="message.text"/>  
    <test parameter="[text]" condition="string.contains" value="INVITE tel:1234 SIP/2.0"/>
    <test parameter="[text]" condition="string.contains" value="------=_Part_0"/>
    <test parameter="[text]" condition="string.contains" value="------=_Part_0--"/>
    
    <parameter name="[binary]" operation="protocol.setFromMessage" value="message.binary"/>  
    <test parameter="[binary]" condition="binary.contains" value="494e5649"/>
    
    <parameter name="[firstline]" operation="protocol.setFromMessage" value="firstline"/>
    <test parameter="[firstline]" condition="string.equals" value="INVITE tel:1234 SIP/2.0"/>
    
    <parameter name="[firstlineVersion]" operation="protocol.setFromMessage" value="firstline.Version"/>
    <test parameter="[firstlineVersion]" condition="string.equals" value="SIP/2.0"/>
    
    <parameter name="[firstlineMethod]" operation="protocol.setFromMessage" value="firstline.Method"/>
    <test parameter="[firstlineMethod]" condition="string.equals" value="INVITE"/>
    
    <parameter name="[firstlineURI]" operation="protocol.setFromMessage" value="firstline.URI"/>
    <test parameter="[firstlineURI]" condition="string.equals" value="tel:1234"/>
    
    <parameter name="[firstlineStatusCode]" operation="protocol.setFromMessage" value="firstline.statusCode"/>
    <test parameter="[firstlineStatusCode]" condition="list.isempty"/>
    
    <parameter name="[firstlineReasonPhrase]" operation="protocol.setFromMessage" value="firstline.reasonPhrase"/>
    <test parameter="[firstlineReasonPhrase]" condition="list.isempty"/>
    
    <parameter name="[headers]" operation="protocol.setFromMessage" value="header"/>
    <parameter name="[headersIndex]" operation="findMatches" value="[headers]" value2="^From.*"/>
    <test parameter="[headersIndex]" condition="number.equals" value="2"/> <!-- for jre  before or equals 1.7 -->
    <parameter name="[headersSize]" operation="list.size" value="[headers]"/>
    <test parameter="[headersSize]" condition="string.equals" value="28"/>
    <test parameter="[headers(0)]" condition="string.contains" value="To:"/>  <!-- for jre  before or equals 1.7 -->
    
    <parameter name="[others]" operation="protocol.setFromMessage" value="header.Others"/>
    <parameter name="[othersIndex]" operation="findMatches" value="[others]" value2="^Content-Length.*"/>
    <parameter name="[othersSize]" operation="list.size" value="[others]"/>
    <!-- test non pertinent car l'ordre des headers n'est pas celui du message -->
    <!-- il faudrait utiliser une TreeMap au lieu d'une Hashmap java -->
    <test parameter="[othersSize]" condition="string.equals" value="19"/>
    <test parameter="[others(9)]" condition="string.contains" value="Content-Type:"/> <!-- for jre  after 1.7 -->        
    
