<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/scenario.xsd -->
<!--  name="Send 3GPP CX Multimedia Authentication request (MAR/MAA)" -->
<scenario>

    <call name="diameter.openChannel_SendCER_RecCEA"/>
                                
    <parameter name="[newSessionId]" operation="set" value="11112222333344445555"/>
    <sendMessageDIAMETER name="SendRequest" remoteURL="diameter://[remoteDIAM]" destScenario="server">
        <header request="true" command="9999999" applicationId="2999999999"
                hopByHop="123456789" endToEnd="987654321"/>		
        <avp code="263" value="[newSessionId]" type="OctetString" mandatory="true"/>
        <avp code="264" value="1.1.1.1" type="OctetString" mandatory="true"/>
        <avp code="296" value="[localRealmDIAM]" type="OctetString" mandatory="true"/>
        <avp code="293" value="2.2.2.2" type="OctetString" mandatory="true"/>
        <avp code="283" value="[remoteRealmDIAM]" type="OctetString" mandatory="true"/>
        
        <avp code="260" type="Grouped" mandatory="true">
            <avp code="266" value="999999999" type="Integer32" mandatory="true"/>
            <avp code="258" value="1999999999" type="Integer32" mandatory="true"/>
        </avp>
        <avp code="2999999990" value="2999999999" type="Integer32" mandatory="true"/>
        <avp code="2999999991" value="sip:+33875610603@sip.france.fr" vendorId="[vendorID]" type="OctetString" mandatory="true"/>
        <avp code="2999999992" value="3.14e0" type="Float32" mandatory="true"/>
        <avp code="2999999993" vendorId="[vendorID]" type="Grouped" mandatory="true">
            <avp code="2999999994" value="0" vendorId="[vendorID]" type="Integer32" mandatory="true"/>
            <avp code="2999999995" value="2" type="Integer32" mandatory="true"/>
            <avp code="2999999996" value='Digest username=\"33875631975@sip.pfdev.fr\",realm=\"sip.pfdev.fr\",cnonce=\"40b18ccf\",nc=00000001,qop=auth,uri=\"sip:sip.pfdev.fr\",nonce=\"21bc27679ddac8fd6c0088817c8ae385\",response=\"38179d99cc34ce0ad002ac4d70e37c0a\",algorithm=MD5' vendorId="[vendorID]" type="OctetString"/>
            <avp code="2999999977" value="INVITE" type="OctetString"/>
        </avp>
    </sendMessageDIAMETER>

    <receiveMessageDIAMETER name="Receive Answer" request="false" type="Unknown:9999999" result="1999999999">
    </receiveMessageDIAMETER>
    
    <!-- check message data -->
    <parameter name="[commandCode]" operation="protocol.setFromMessage" value="header.command"/>
    <test parameter="[commandCode]" condition="string.equals" value="Unknown:9999999"/>
    <parameter name="[applicationId]" operation="protocol.setFromMessage" value="header.applicationId"/>
    <test parameter="[applicationId]" condition="string.equals" value="Unknown:2999999999"/>
    <parameter name="[binary]" operation="protocol.setFromMessage" value="message.binary"/>    
    <test parameter="[binary]" condition="binary.equals" value=""/>


</scenario>
